op_push1 => 0x01
op_push4 => 0x04
op_ecall => 0x13
callno_print => 0x02
callno_exit => 0x01

push1 x =>
  !op_push1
  !{x}

push4 x =>
  !op_push4
  !{x}

ecall => !op_ecall

print value =>
  push4 {value}
  push1 callno_print
  ecall

exit code =>
  push1 {code}
  push1 callno_exit
  ecall

print "Hello, World"
exit 0

